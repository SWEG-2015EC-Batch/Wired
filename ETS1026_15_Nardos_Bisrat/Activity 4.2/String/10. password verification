#include <iostream>
#include <string>

using namespace std;

int main() {
    string storedPassword = "selam"; 
    string enteredPassword;

    cout << "Enter the password: ";
    cin >> enteredPassword;

    if (enteredPassword == storedPassword) {
        cout << "Password is correct!" << endl;
    } else {
        cout << "Incorrect password." << endl;
    }

    return 0;
}
